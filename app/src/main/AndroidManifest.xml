<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drivoappui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DrivoAppUI">
        <activity
            android:name="com.example.drivoapp.Location"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.ForgotPassword"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.Payment"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.Booking"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.CarDetail"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.Register"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.Login"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.WelcomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.GetStarted"
            android:exported="false" />
        <activity
            android:name="com.example.drivoapp.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DrivoAppUI.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.drivoapp.SplashScreen"
            android:exported="false"
            android:theme="@style/Theme.DrivoAppUI.NoActionBar" />
    </application>

</manifest>